<%= form_for(:session,url:  sessions_path) do |f| %>
    <div class="form-group">
        <%= f.label :email%>
        <%= f.email_field :email, class: 'form-control' %>
    </div>

    <div class="form-group">
        <%= f.label :password%>
        <%= f.password_field :password, class: 'form-control' %>
    </div>
    
    <%= f.submit "Log in", class: 'btn btn-success btn-block mb-4'%>
<%end%>